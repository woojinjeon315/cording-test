def solution(data):
    d=0
    f=0
    a=bin(data)[2:].replace('1','A').replace('0','B')
    b=a.replace('A','0').replace('B','1')
    
    while b == bin(d)
        d+=1
        f=d
    return f

print(solution(5))
print(solution(9))
print(solution(10))

def solution(data):

    a=bin(data)[2:]

    b=a.rjust(10,'0')

    c=b.replace('1','A').replace('0','B')
    d=c.replace('A','0').replace('B','1')
    return int(d,2)

print(solution(5))
print(solution(9))
print(solution(10))
