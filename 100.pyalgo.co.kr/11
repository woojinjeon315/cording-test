def solution(data):
    s=0


    for i in data:
        if sum(i[0:3]) >= 240:
            print(i)
            s += 1
    
    
    
    
    return s

solution([[92, 85, 97], [30, 21, 60], [90, 99, 98], [0, 0, 0], [81, 80, 88, 83]])
# [[98, 92, 85], [95, 32, 51], [98, 98, 51]]
# [[92, 85, 97], [30, 21, 60], [90, 99, 98], [0, 0, 0], [81, 80, 88, 83]]
